            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Globalization\en-US\en-US.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <vm:DifficultyOffsetValueConverter x:Key="DifficultyOffsetValueConverter" />
