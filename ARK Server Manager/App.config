  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <variable name="logDir" value=""/>
    <targets>
      <target name="debug"
              xsi:type="Debugger"
              layout="${time} DEBUG: ${message}"
              />
      <target name="error"
              xsi:type="Debugger"
              layout="${time} ERROR: ${message}"
              />
      <target name="debugFile" xsi:type="File"
              fileName="${logDir}/ASM_Debug.log"
              layout="${time} ${message}"
              archiveFileName="${logDir}/ASM_Debug.{#}.log"
              archiveNumbering="DateAndSequence"
              archiveEvery="Day"
              archiveDateFormat="yyyyMMdd"
              />
      <target name="errorFile" xsi:type="File"
              fileName="${logDir}/ServerManager_Error.log"
              layout="${time} ${message}"
              archiveFileName="${logDir}/ServerManager_Error.{#}.log"
              archiveNumbering="DateAndSequence"
              archiveEvery="Day"
              archiveDateFormat="yyyyMMdd"
              />
      <target name="scripts" xsi:type="File"
              fileName="${logDir}/ASM_Scripts.log"
              layout="${time} ${message}"
              archiveFileName="${logDir}/ASM_Scripts.{#}.log"
              archiveNumbering="DateAndSequence"
              archiveEvery="Day"
              archiveDateFormat="yyyyMMdd"
              />
      <target name="statuswatcher" xsi:type="File"
              fileName="${logDir}/ASM_ServerStatus.log"
              layout="${time} ${message}"
              archiveFileName="${logDir}/ASM_ServerStatus.{#}.log"
              archiveNumbering="DateAndSequence"
              archiveEvery="Day"
              archiveDateFormat="yyyyMMdd"
              />
      <target name="taskscheduler" xsi:type="File"
              fileName="${logDir}/ASM_TaskScheduler.log"
              layout="${time} ${message}"
              archiveFileName="${logDir}/ASM_TaskScheduler.{#}.log"
              archiveNumbering="DateAndSequence"
              archiveEvery="Day"
              archiveDateFormat="yyyyMMdd"
              />
    </targets>
    <rules>
      <!--<logger name="*" maxlevel="debug" writeTo="debug"/>
      <logger name="*" minlevel="error" writeTo="error"/>-->
      <logger name="*" maxlevel="debug" writeTo="debugFile"/>
      <logger name="*" minlevel="error" writeTo="errorFile"/>
      <logger name="ARK_Server_Manager.Lib.ScriptUtils"         minlevel="debug" writeTo="scripts"/>
      <logger name="ARK_Server_Manager.Lib.ServerStatusWatcher" minlevel="debug" writeTo="statuswatcher"/>
      <logger name="ARK_Server_Manager.Lib.TaskSchedulerUtils"  minlevel="debug" writeTo="taskscheduler"/>
    </rules>
  </nlog>
