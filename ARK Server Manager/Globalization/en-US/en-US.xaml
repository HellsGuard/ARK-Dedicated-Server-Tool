    <sys:String x:Key="ServerSettings_WildDamageOptionLabel">Wild Damage</sys:String>
    <sys:String x:Key="ServerSettings_WildDamageOptionTooltip">Show/Hide the wild dino damage multiplier column.</sys:String>
