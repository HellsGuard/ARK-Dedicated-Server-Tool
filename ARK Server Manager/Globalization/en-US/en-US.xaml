    <sys:String x:Key="ServerSettings_SpawnMultipliersOptionTooltip">Show/Hide the spawn multiplier columns.</sys:String>
    <sys:String x:Key="ServerSettings_TamedDamageOptionLabel">Tamed Damage</sys:String>
